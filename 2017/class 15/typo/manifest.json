{

  "manifest_version": 2,

  "version": "1.0",

  "name": "Typo",
  "description": "Typo is a typing game that will test your typing skills.",

  "icons": {
      "48": "48.png",
      "96": "96.png",
      "128": "128.png"
  },

  "background": {
    "page": "background.html"
  },

  "browser_action": {
    "default_icon": {
      "128": "128.png"
    }
  },

  "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
  ],

  "applications": {
    "gecko": {
      "id": "juraj.masiar@oa.Typo",
      "strict_min_version": "60.0"
    }
  }

}
